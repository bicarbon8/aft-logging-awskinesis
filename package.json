{
  "name": "aft-logging-awskinesis",
  "version": "2.0.0",
  "description": "Automated Functional Testing (AFT) logging plugin package supporting test reporting to AWS Kinesis Firehose",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "npm run clean && tsc --declaration",
    "pretest": "npm run build",
    "test": "./node_modules/.bin/jasmine JASMINE_CONFIG_PATH=./jasmine.json",
    "coverage": "nyc npm run test"
  },
  "keywords": [
    "aft",
    "automation",
    "e2e",
    "integration",
    "functional",
    "aws",
    "kinesis-firehose"
  ],
  "author": "Jason Holt Smith <bicarbon8@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "aft-core": "^2.0.0",
    "aws-sdk": "2.552.0",
    "es6-promise": "^4.2.8"
  },
  "devDependencies": {
    "@types/node": "^12.7.12",
    "@types/jasmine": "^3.4.4",
    "jasmine": "^3.5.0",
    "typescript": "^3.6.4"
  }
}
